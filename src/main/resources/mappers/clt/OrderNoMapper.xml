<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fssoft.freight.dao.persistence.dao.OrderNoMapper">

    <select id="findNumByTime" resultType="java.lang.Integer">
        SELECT num FROM dz_order_no WHERE time = #{time} ORDER BY num DESC limit 1
    </select>

</mapper>